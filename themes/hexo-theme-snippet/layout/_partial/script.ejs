<% if(theme.carousel.carousel){ %>
    <script>
        var oDiv5=document.getElementById('div5');
        var oUl=oDiv5.getElementsByTagName('ul')[0];
        var oLi=oUl.getElementsByTagName('li');
        document.addEventListener('DOMContentLoaded',function(){
        	oUl.innerHTML+=oUl.innerHTML;
        	oUl.style.width=oLi[0].offsetWidth*oLi.length +'px';
        	var speed = 16;
        	function move(){
        		//循环滚动
        		if(oUl.offsetLeft<-oUl.offsetWidth/2) {
        		    // offsetLeft有可能是负的，此处的offsetLeft起初是0，再移动就是负的
        			oUl.style.left=speed+'px';
        		} else if (oUl.offsetLeft>0) {
        			oUl.style.left=-oUl.offsetWidth/2+speed+'px';
        		}
                oUl.style.left=oUl.offsetLeft+speed+'px';
        	}
        	var timer=setInterval(move,200);
        	oDiv5.onmouseover=function() {
        		clearInterval(timer);
        	}
        	oDiv5.onmouseout=function() {
        		//setInterval(move,30);
        		timer=setInterval(move,200);
        	}
        });
    </script>
<% } %>

<% if(theme.tagcloud.tag3d){ %>
    <%- js('assets/tagcanvas.min.js?rev=2.9') %>
    <script>
        var tagOption = {
            textColour: '<%= theme.tagcloud.textColour %>', // 字体颜色
            outlineMethod: '<%= theme.tagcloud.outlineMethod %>', // 选中模式
            outlineColour: '<%= theme.tagcloud.outlineColour %>', // 选中模式的颜色
            interval: <%= theme.tagcloud.interval %> || 30, // 动画帧之间的时间间隔，值越大，转动幅度越大
            textHeight: 13,
            outlineRadius: 3,
            freezeActive: <%= theme.tagcloud.freezeActive %> || '', // 选中的标签是否继续滚动
            frontSelect: <%= theme.tagcloud.frontSelect %> || '', // 不选标签云后部的标签
            initial: [0.1, -0.1],
            depth: 0.5,
            decel: 0.95,
            maxSpeed: 0.03,
            reverse: <%= theme.tagcloud.reverse %> || '', // 是否反向触发
            fadeIn: 500, // 进入动画时间
            wheelZoom: <%= theme.tagcloud.wheelZoom %> || '' // 是否启用鼠标滚轮
        }
        TagCanvas.Start('tag-cloud-3d','',tagOption);
    </script>
<% } %>

